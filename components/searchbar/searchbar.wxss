/* components/searchbar/searchbar.wxss */
@import '../../style/weui.wxss';

.searchbar-result{
    margin-top: 0;
    font-size: 14px;
}
.searchbar-result:before{
    display: none;
}

.fixed {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
}

/* 复写weui样式 */
.weui-cell{
    padding: 12px 15px 12px 35px;
}

.weui-search-bar {
    border: none;
    background-color: #F3F3F3;
    color: #999999;
    padding: 10px 15px;
}

.weui-search-bar__form {
    border-radius: 0;
    border: none;
}