/* pages/kol/kol-share.wxss */
@import "../promotion/promotion-count.wxss";
.flex{
    margin: 0;
}
.icon-32{
    width: 32rpx;
    height: 32rpx;
    transition: transform .4s;
    transform: rotate(0deg);
}
.icon-open{
    transform: rotate(180deg);
}
.t-c{
    text-align: center!important;
}
.left{
    width: 100rpx;
}
.center{
    width: 300rpx;
}
.right{
    width: 200rpx;
}
.flex-h100{
    height: 100rpx;
}
.flex-h60{
    height: 60rpx;
}
.weui-demo-dialog{
    visibility:hidden;
    opacity:0;
    transition:all .3s;
}
.weui-demo-dialog_show{
    visibility:visible;
    opacity:1;
}
.fixed{
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 200rpx;
}
.container{
    padding-bottom: 200rpx;
}
.ml48{
    margin-left: 48rpx;
}
.mr48{
    margin-right: 48rpx;
}