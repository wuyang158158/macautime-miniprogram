/* pages/views/order-detail.wxss */
.pb180{
    padding-bottom: 150rpx;
}
.pb220{
    padding-bottom: 220rpx;
}
.title{
    border-radius:16rpx;
    overflow: hidden;
}
.cover-image{
    width: 100rpx;
    height: 100rpx;
    border-radius:16rpx;
}
.h-100{
    height: 100rpx;
}
.flex-column{
    justify-content: space-between;
    align-items: flex-start;
}
.w-0{
    width: 0;
}
.h-100 .view{
    width: 100%;
}
button[type=call]{
    color: rgba(0,166,83,1);
    background-color: rgba(255,255,255,1);
}
.button-hover[type=call]{
    color: rgba(0,166,83, 0.6);
    background-color: rgb(255,255,255, 0.6);
}
button[type=contact]{
    color: rgba(34,34,34,1);
    background-color: rgba(255,255,255,1);
}
.button-hover[type=contact]{
    color: rgba(34,34,34, 0.6);
    background-color: rgb(255,255,255, 0.6);
}
.weui-btn + .weui-btn{
    margin: 0;
}
.v-btn view{
    height: 99rpx;
}
.v-btn view button{
    height: 100rpx;
    line-height: 100rpx;
}
.evaluate{
    width: 100%;
    height: 120rpx;
    position: fixed;
    bottom: 0;
    left: 0;
}
.w-132{
    width: 132rpx;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 26rpx;
}